foreach top [::InstallJammer::Grab stack] {
foreach w [::InstallJammer::GetAllWidgets $top] {
set c [winfo class $w]
if {$c eq "Button" || $c eq "TButton"} {
if {[$w cget -text] eq $button} {
set found 1
$w invoke
break
}
}
}
if {$found} { break }
}
}

